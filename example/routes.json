{
  "routes": [
    {
      "version": "1.0",
      "url": {
        "prefix": "list/{urlName}"
      },
      "request": {
        "method": "get"
      },
      "name": "test",
      "description": "A test route",
      "dispatcher": "dispatcher",
      "response": {
        "parser": "ldl.response.parser.template",
        "template": {
          "engine": "template.engine.php",
          "codes": {
            "200-299": {
              "file": "success.php"
            },
            "401": {
              "file": "forbidden.php"
            }
          }
        }
      }
    },
    {
      "version": "1.0",
      "url": {
        "prefix": "error"
      },
      "request": {
        "method": "get"
      },
      "name": "test",
      "description": "A test route",
      "dispatcher": "dispatcher",
      "response": {
        "parser": "ldl.response.parser.template",
        "template": {
          "engine": "template.engine.php",
          "codes": {
            "any": {
              "file": "error.php"
            }
          }
        }
      }
    }
  ]
}